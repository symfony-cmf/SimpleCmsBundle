{
    "name": "symfony-cmf/simple-cms-bundle",
    "type": "symfony-bundle",
    "description": "A simple CMS bundle based on the Symfony CMF",
    "keywords": ["Symfony CMF", "CMS"],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/SimpleCmsBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.2",
        "symfony-cmf/core-bundle": "~1.1",
        "symfony-cmf/routing-bundle": "1.2.*",
        "symfony-cmf/menu-bundle": "~1.1",
        "symfony-cmf/content-bundle": "~1.1",
        "doctrine/phpcr-bundle": "~1.1",
        "doctrine/phpcr-odm": "~1.1"
    },
    "require-dev": {
        "symfony-cmf/testing": ">=1.0.0,<1.2-dev",
        "matthiasnoback/symfony-dependency-injection-test": "0.*",
        "matthiasnoback/symfony-config-test": "0.*",
        "sonata-project/doctrine-phpcr-admin-bundle": ">=1.0.0,<1.2-dev",
        "symfony/monolog-bundle": "~2.2"
    },
    "suggest": {
        "symfony-cmf/create-bundle": "1.1.*",
        "sonata-project/doctrine-phpcr-admin-bundle": "1.1.*"
    },
    "autoload": {
        "psr-0":{
            "Symfony\\Cmf\\Bundle\\SimpleCmsBundle": ""
        }
    },
    "conflicts": {
        "jms/di-extra-bundle": "<1.3"
    },
    "target-dir": "Symfony/Cmf/Bundle/SimpleCmsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
